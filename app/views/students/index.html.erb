<h1 id="page_title">Students</h1>
<section class="clear" id="works">
	<div class="block">
		<div class="stack">
			<% @works.each_with_index do |work, index| %>
				<% @shift = 5 * index %>
				<div class="work" style="bottom: <%= @shift %>px; left: <%= @shift %>px;">
					<h3><%= work.name %></h3>
					<div class="image_wrapper">
						<%= image_tag work.image.url(:medium) %>
					</div>
				</div>
			<% end %>
		</div>
	</div>
</section>