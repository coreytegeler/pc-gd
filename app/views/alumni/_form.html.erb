<%= f.label(:name) %>
<%= f.text_field(:name) %>

<% @present = Time.now.year %>
<% @past = 1972 %>
<%= f.label(:year) %>
<%= f.select(:year, @past..@present, {include_blank: 'Select'}) %>

<%= f.label("Website") %>
<%= f.text_field(:url) %>

<%= f.label(:position) %>
<%= f.select(:position, 1..@count) %>

<%= f.label(:visible) %>
<%= f.check_box(:visible, :checked => true) %>

